            <latitude>0</latitude>
            <altitude>9000000</altitude>
            <heading>0</heading>
            <tilt>0</tilt>
            <gx:fovy>35</gx:fovy>
            <range>9000000</range>
            <altitudeMode>absolute</altitudeMode>
        </LookAt>
        <styleUrl>#__managed_style_05358BB35E1F0906FAEE</styleUrl>
        <MultiGeometry>
            <Polygon>
